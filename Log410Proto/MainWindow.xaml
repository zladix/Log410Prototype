<Window x:Class="Log410Proto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Log410Proto"
        mc:Ignorable="d"
        Title="MainWindow" Height="390.093" Width="661.533">
    <Grid>
        <RadioButton x:Name="radioButton" Content="2D" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Checked="radioButton_Checked"/>
        <RadioButton x:Name="radioButton1" Content="3D" HorizontalAlignment="Left" Margin="65,11,0,0" VerticalAlignment="Top" Checked="radioButton1_Checked" RenderTransformOrigin="0.5,0.5">
            <RadioButton.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.519"/>
                    <TranslateTransform/>
                </TransformGroup>
            </RadioButton.RenderTransform>
        </RadioButton>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="329" Margin="0,30,0,0" Stroke="Black" VerticalAlignment="Top" Width="526"/>
        <CheckBox x:Name="checkBox_Static" Content="Statique" HorizontalAlignment="Left" Margin="541,102,0,0" VerticalAlignment="Top" Height="16" Width="103" Checked="checkBox_Static_Checked" IsChecked="True" Unchecked="checkBox_Static_Unchecked"/>
        <CheckBox x:Name="checkBox_Dynamic" Content="Dynamique" HorizontalAlignment="Left" Margin="541,124,0,0" VerticalAlignment="Top" Height="16" Width="103" Checked="checkBox_Dynamic_Checked" IsChecked="True" Unchecked="checkBox_Dynamic_Unchecked"/>
        <CheckBox x:Name="checkBox_WorldMap" Content="World Map" HorizontalAlignment="Left" Margin="541,145,0,0" VerticalAlignment="Top" Height="16" Width="103" Checked="checkBox_WorldMap_Checked" IsChecked="True" Unchecked="checkBox_WorldMap_Unchecked"/>
        <Button x:Name="createObjButton" Content="Create Object" HorizontalAlignment="Left" Margin="531,329,0,0" VerticalAlignment="Top" Width="86" Click="createObjButton_Click"/>
        <Label x:Name="label" Content="Afficher les objets" HorizontalAlignment="Left" Margin="531,60,0,0" VerticalAlignment="Top" Width="113" FontWeight="Bold" Height="25"/>
        <Ellipse Fill="Red" HorizontalAlignment="Left" Height="100" Margin="95,102,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
        <Rectangle Fill="Yellow" HorizontalAlignment="Left" Height="40" Margin="125,57,0,0" Stroke="Black" VerticalAlignment="Top" Width="40"/>
        <Rectangle Fill="Yellow" HorizontalAlignment="Left" Height="40" Margin="50,135,0,0" Stroke="Black" VerticalAlignment="Top" Width="40"/>
        <Rectangle Fill="Yellow" HorizontalAlignment="Left" Height="40" Margin="200,135,0,0" Stroke="Black" VerticalAlignment="Top" Width="40"/>
        <Rectangle x:Name="createRectangle" Fill="Yellow" HorizontalAlignment="Left" Height="40" Margin="125,207,0,0" Stroke="Black" VerticalAlignment="Top" Width="40" Visibility="Hidden"/>
        <Rectangle Fill="Red" HorizontalAlignment="Left" Height="57" Margin="270,40,0,0" Stroke="Black" VerticalAlignment="Top" Width="210"/>
        <Rectangle Fill="Red" HorizontalAlignment="Left" Height="56" Margin="401,118,0,0" Stroke="Black" VerticalAlignment="Top" Width="104" RenderTransformOrigin="0.5,0.5">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="45" Margin="300,102,0,0" Stroke="Black" VerticalAlignment="Top" Width="45"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="45" Margin="365,102,0,0" Stroke="Black" VerticalAlignment="Top" Width="45"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="45" Margin="365,157,0,0" Stroke="Black" VerticalAlignment="Top" Width="45"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="15" Margin="465,102,0,0" Stroke="Black" VerticalAlignment="Top" Width="15"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="15" Margin="465,124,0,0" Stroke="Black" VerticalAlignment="Top" Width="15"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="15" Margin="465,146,0,0" Stroke="Black" VerticalAlignment="Top" Width="15" x:Name="staticItemCan3"/>
        <Rectangle Fill="#FFFFF300" HorizontalAlignment="Left" Height="28" Margin="285,57,0,0" Stroke="Black" VerticalAlignment="Top" Width="75"/>
        <Ellipse Fill="#FFFFF300" HorizontalAlignment="Left" Height="20" Margin="135,141,0,0" Stroke="Black" VerticalAlignment="Top" Width="20"/>
        <Ellipse Fill="Red" HorizontalAlignment="Left" Height="69" Margin="421,235,0,0" Stroke="Black" VerticalAlignment="Top" Width="69"/>
        <Rectangle x:Name="redCreate" Fill="Red" HorizontalAlignment="Left" Height="40" Margin="125,207,0,0" Stroke="Black" VerticalAlignment="Top" Width="40" Visibility="Hidden"/>
        <Label x:Name="chairLabel" Content="Label" HorizontalAlignment="Left" Margin="125,217,0,0" VerticalAlignment="Top" Visibility="Hidden"/>


    </Grid>
</Window>
