<Window x:Class="Log410Proto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Log410Proto"
        mc:Ignorable="d"
        Title="MainWindow" Height="390.093" Width="661.533">
    <Grid>
        <RadioButton x:Name="radioButton" Content="2D" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Checked="radioButton_Checked"/>
        <RadioButton x:Name="radioButton1" Content="3D" HorizontalAlignment="Left" Margin="65,11,0,0" VerticalAlignment="Top" Checked="radioButton1_Checked" RenderTransformOrigin="0.5,0.5">
            <RadioButton.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.519"/>
                    <TranslateTransform/>
                </TransformGroup>
            </RadioButton.RenderTransform>
        </RadioButton>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="329" Margin="0,30,0,0" Stroke="Black" VerticalAlignment="Top" Width="526"/>
        <CheckBox x:Name="checkBox" Content="CheckBox" HorizontalAlignment="Left" Margin="547,61,0,0" VerticalAlignment="Top" Height="18" Width="78"/>
        <CheckBox x:Name="checkBox_Copy" Content="CheckBox" HorizontalAlignment="Left" Margin="547,84,0,0" VerticalAlignment="Top" Height="18" Width="78"/>
        <CheckBox x:Name="checkBox_Copy1" Content="CheckBox" HorizontalAlignment="Left" Margin="547,107,0,0" VerticalAlignment="Top" Height="18" Width="78"/>
        <CheckBox x:Name="checkBox_Copy2" Content="CheckBox" HorizontalAlignment="Left" Margin="547,130,0,0" VerticalAlignment="Top" Height="18" Width="78"/>
        <Button x:Name="createObjButton" Content="Button" HorizontalAlignment="Left" Margin="531,329,0,0" VerticalAlignment="Top" Width="75" Click="createObjButton_Click"/>

    </Grid>
</Window>
